{
  "name": "devoakroot-school-app",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:staging": "ng serve --configuration=staging ",
    "build": "ng build --base-href ./",
    "test": "npm run lint && ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "watch": "ng test --browsers ChromeHeadless --watch",
    "build:prod": "ng build --prod --build-optimizer --vendor-chunk --common-chunk",
    "build:staging": "ng build --configuration=staging --build-optimizer --vendor-chunk --common-chunk",
    "build:demo": "ng build --configuration=demo --build-optimizer --vendor-chunk --common-chunk",
    "clean": "rimraf ./dist/",
    "analyze": "npm run clean && npm run build:prod -- --stats-json && webpack-bundle-analyzer ./dist/stats.json"
  },
  "private": true,
  "dependencies": {
